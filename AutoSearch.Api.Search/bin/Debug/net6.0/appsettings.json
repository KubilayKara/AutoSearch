{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SearchSettings": {
    "DesiredUrl": "www.infotrack.co.uk",
    "SearchEngines": [
      {
        "IsDefault": true,
        "Id": 1,
        "Title": "Google",
        "URL": "http://www.google.co.uk/search?num=[num]&q=[keywords]",
        "Regex": "egMi0 kCrYT(.+?)sa=U"
      },
      {
        "Id": 2,
        "Title": "Bing",
        "URL": "www.bing.com\tsearch?q=[keywords]",
        "Regex": "((<cite>)(.+?)(</cite>))"
      }
    ]
  }

}
